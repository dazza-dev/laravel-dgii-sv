{
  "name": "dazza-dev/laravel-dgii-sv",
  "description": "Paquete Laravel para facturación electrónica DGII (El Salvador).",
  "keywords": [
    "dgii",
    "dgii-laravel"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "dazza-dev/dgii-sv": "^1.0",
    "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "larastan/larastan": "^2.0",
    "laravel/pint": "^1.18",
    "orchestra/testbench": "^8.27"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Andres Daza",
      "email": "adaza90@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "DazzaDev\\LaravelDgiiSv\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DazzaDev\\LaravelDgiiSv\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DazzaDev\\LaravelDgiiSv\\LaravelDgiiSvServiceProvider"
      ],
      "aliases": {
        "LaravelDgiiSv": "DazzaDev\\LaravelDgiiSv\\Facades\\LaravelDgiiSv"
      }
    }
  }
}
